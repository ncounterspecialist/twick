{
  "name": "@twick/render-server",
  "version": "0.15.0",
  "license": "https://github.com/ncounterspecialist/twick/blob/main/LICENSE.md",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "twick-render-server": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test:render": "tsx src/test-render.ts",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@twick/2d": "^0.15.21",
    "@twick/core": "^0.15.21",
    "@twick/ffmpeg": "^0.15.21",
    "@twick/renderer": "^0.15.21",
    "@twick/ui": "^0.15.21",
    "@twick/visualizer": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
