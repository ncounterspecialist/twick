    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@twick/timeline": "workspace:*",
    "@twick/live-player": "workspace:*",
    "@twick/video-editor": "workspace:*",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.2",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.7.3"
  }
