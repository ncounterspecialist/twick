{
  "name": "@twick/studio",
  "version": "0.14.11",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/studio.css": "./dist/studio.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "dist"
